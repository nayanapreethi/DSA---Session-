public class Main {
    public static void main(String[] args) {

        int[] array = {3,6,7,9,12};
        int sum = 0, product = 1;
        
        for(int i = 0;i<array.length;i++){
            sum = sum + array[i];
            product = product * array[i];
        }
        System.out.println("Sum: " + sum + " Product: " + product);
    }
}
